package com.classes;

public class Carta {

	private int valor;
	private NAIPE naipe;
	private boolean usada;

	public Carta(int valor, NAIPE naipe) {
		this.valor = valor;
		this.naipe = naipe;
		this.usada = false;
	}
	
	@Override
	public String toString(){
		return String.valueOf(valor)+" de "+naipe.name();		
	}

}
