package com.classes;

public class Carta {

	private int valor;
	private NAIPE naipe;
	private boolean usada;

	public Carta(int valor, NAIPE naipe) {
		this.valor = valor;
		this.naipe = naipe;
		this.usada = false;
	}
	
	@Override
	public String toString(){
		return valorToString() +" de " + naipe.name();		
	}

	private String valorToString() {
		String valorTemp = new String();
		return valorTemp;
	}

}
